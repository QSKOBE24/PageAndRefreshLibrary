<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qskobe24.mylistview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="14" />
    <!-- 访问网络的权限 -->
	<uses-permission android:name="android.permission.INTERNET"/>
	<!-- 在SDCard中创建与删除文件权限 -->
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
	<!-- 往SDCard写入数据权限 -->
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.qskobe24.mylistview.activity.MainActivity">
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
          <activity
            android:name="com.qskobe24.mylistview.activity.ProductEarningActivity">
        </activity>
            <activity
            android:name="com.qskobe24.mylistview.activity.AllEarningActivity">
        </activity>
          <activity
            android:name="com.qskobe24.mylistview.activity.DealFlowActivity">
        </activity>
          <activity
            android:name="com.qskobe24.mylistview.activity.CancelDepositActivity">
        </activity>
        
        <activity
            android:name="com.qskobe24.mylistview.activity.SecondActivity">
        </activity>
         <activity
            android:name="com.qskobe24.mylistview.activity.ThirdActivity">
        </activity>
         <activity
            android:name="com.qskobe24.mylistview.activity.ForthActivity">
        </activity>
         <activity
            android:name="com.qskobe24.mylistview.activity.FifthActivity">
        </activity>
         <activity
            android:name="com.qskobe24.mylistview.activity.SixthActivity">
        </activity>
         <activity
            android:name="com.qskobe24.mylistview.activity.SeventhActivity">
        </activity>
    </application>

</manifest>
